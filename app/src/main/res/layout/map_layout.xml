<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
                                                   xmlns:app="http://schemas.android.com/apk/res-auto"
                                                   xmlns:tools="http://schemas.android.com/tools"
                                                   android:layout_width="match_parent"
                                                   android:layout_height="match_parent">

    <ImageView
            android:id="@+id/imageView"
            android:layout_width="0dp"
            android:layout_height="0dp"
            android:background="@drawable/map"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent" app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintTop_toTopOf="parent" app:layout_constraintHorizontal_bias="0.363"
            app:layout_constraintVertical_bias="0.35" tools:ignore="ImageContrastCheck"
            android:contentDescription="@string/mapMapDescription"/>
    <ImageButton
            android:onClick="switchToMainActivity"
            android:contentDescription="@string/goBackString"
            android:layout_width="90dp"
            android:layout_height="81dp" app:srcCompat="@drawable/icon_back" android:id="@+id/backButton"
            android:background="#00FFFFFF"
            android:scaleType="centerInside"
            app:layout_constraintTop_toTopOf="parent" app:layout_constraintStart_toStartOf="parent"
            android:layout_marginTop="16dp" android:layout_marginStart="16dp" tools:ignore="ImageContrastCheck"/>
    <ImageButton
            android:onClick="viewZooAction"
            android:contentDescription="@string/mapPinIconDescription"
            android:layout_width="58dp"
            android:layout_height="57dp" app:srcCompat="@drawable/epingle" android:id="@+id/zoo1"
            android:scaleType="centerInside" android:background="#00FFFFFF"
            app:layout_constraintEnd_toEndOf="@+id/imageView" app:layout_constraintStart_toStartOf="@+id/imageView"
